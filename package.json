{
  "name": "owen",
  "version": "1.0.0",
  "description": "owen",
  "main": "src/main.js",
  "scripts": {
    "watch": "electromon src/main.js",
    "start": "electron src/main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "owen",
    "lint": "eslint ./src/**/*.js --fix"
  },
  "build": {
    "appId": "owen",
    "mac": {
      "category": "owen"
    }
  },
  "repository": {
    "type": "git",
    "url": "owen"
  },
  "keywords": [
    "owen"
  ],
  "author": "owen",
  "license": "ISC",
  "dependencies": {
    "discord-rpc": "^3.1.1",
    "electron-updater": "^4.3.1"
  },
  "devDependencies": {
    "electron-builder": "^22.7.0",
    "electron": "^9.0.5",
    "eslint": "^7.4.0",
    "electromon": "^1.0.10"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "rules": {
      "indent": [
        "warn",
        4
      ],
      "linebreak-style": [
        "warn",
        "unix"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": "off"
    }
  }
}
